{
  "routes": [
    {
      "route": "/profile",
      "allowedRoles": [
        "authenticated"
      ]
    },
    {
      "route": "/login",
      "serve": "/.auth/login/google"
    },
    {
      "route": "/logout",
      "serve": "/.auth/logout"
    }
  ],
  "platformErrorOverrides": [
    {
      "errorType": "NotFound",
      "serve": "/custom-404.html"
    },
    {
      "errorType": "Unauthenticated",
      "statusCode": 301,
      "serve": "/login"
    }
  ]
}